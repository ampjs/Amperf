{
  "name": "@ampersarnie/amperf",
  "version": "0.1.0",
  "description": "Amperf",
  "main": "./lib/AmperfNode.js",
  "options": {},
  "bin": {
    "amperf": "./bin/amperf"
  },
  "scripts": {
    "mocha": "node_modules/.bin/mocha --growl test/tests.js",
    "mocha:report": "node_modules/.bin/mocha --reporter markdown test/tests.js > test/RESULTS.md; echo $(($(date +'%s * 1000 + %-N / 1000000'))) >> test/RESULTS.md",
    "compile": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "devbuild": "npm run compile; npm run mocha",
    "prepublish": "npm run compile; npm run mocha; npm run mocha:report",
    "lint": "eslint src/*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "es6",
    "javascript",
    "performance",
    "perf"
  ],
  "author": "Paul Taylor <hello@ampersandwich.co.uk> (http://ampersandwich.co.uk)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.2"
  },
  "dependencies": {
    "@ampersarnie/outputs": "^0.1.0",
    "@ampersarnie/implements": "^0.1.2"
  }
}
